<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="YouTubePlayerSeekBar">
        <attr name="fontSize" format="dimension" />
        <attr name="color" format="color" />
    </declare-styleable>

    <declare-styleable name="YouTubePlayerView">
        <attr name="enableAutomaticInitialization" format="boolean" />
        <attr name="useWebUi" format="boolean" />
        <attr name="enableLiveVideoUi" format="boolean" />
        <attr name="videoId" format="string" />
        <attr name="autoPlay" format="boolean" />
        <attr name="handleNetworkEvents" format="boolean" />
        <attr name="showYouTubeButton" format="boolean" />
        <attr name="showFullScreenButton" format="boolean" />
        <attr name="showVideoCurrentTime" format="boolean" />
        <attr name="showVideoDuration" format="boolean" />
        <attr name="showSeekBar" format="boolean" />
    </declare-styleable>
</resources>